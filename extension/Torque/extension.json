{
  "name": "Torque",
  "version": "0.2.2",
  "author": [
    "Open Tech Strategies"
  ],
  "license-name": "GPL-2.0-or-later",
  "url": "https://github.com/OpenTechStrategies/torque",
  "descriptionmsg": "torque-desc",
  "type": "api",
  "requires": {
    "MediaWiki": ">= 1.35.0"
  },
  "MessagesDirs": {
    "Torque": [
      "i18n"
    ]
  },
  "AvailableRights": [
    "torque-admin",
    "torque-edit"
  ],
  "AutoloadClasses": {
    "Torque": "include/Torque.php",
    "TorqueConfig": "include/TorqueConfig.php",
    "TorqueUserLookup": "include/TorqueUserLookup.php",
    "TorqueQuery": "include/api/TorqueQuery.php",
    "TorqueSubmitEdit": "include/api/TorqueSubmitEdit.php",
    "TorqueUploadCollection": "include/api/TorqueUploadCollection.php",
    "TorqueUploadToc": "include/api/TorqueUploadToc.php",
    "TorqueUploadAttachment": "include/api/TorqueUploadAttachment.php",
    "TorqueAttachment": "include/specials/TorqueAttachment.php",
    "TorqueFields": "include/specials/TorqueFields.php",
    "TorqueCsv": "include/specials/TorqueCsv.php",
    "TorqueHooks": "include/hooks/TorqueHooks.php"
  },
  "Hooks": {
    "ParserFirstCallInit": "TorqueHooks::onParserFirstCallInit",
    "BeforeInitialize": "TorqueHooks::onBeforeInitialize",
    "PageContentSaveComplete": "TorqueHooks::onPageContentSaveComplete",
    "SiteNoticeAfter": "TorqueHooks::siteNoticeAfter",
    "SpecialSearchResultsPrepend": "TorqueHooks::onSpecialSearchResultsPrepend",
    "SidebarBeforeOutput": "TorqueHooks::onSidebarBeforeOutput",
    "BaseTemplateToolbox": "TorqueHooks::onBaseTemplateToolbox",
    "BeforePageDisplay": "TorqueHooks::onBeforePageDisplay"
  },
  "APIModules": {
    "torque": "TorqueQuery",
    "torquesubmitedit": "TorqueSubmitEdit",
    "torqueuploadcollection": "TorqueUploadCollection",
    "torqueuploadtoc": "TorqueUploadToc",
    "torqueuploadattachment": "TorqueUploadAttachment"
  },
  "ExtensionMessagesFiles": {
    "TorqueMagic": "Torque.i18n.php"
  },
  "SpecialPages": {
    "TorqueAttachment": "TorqueAttachment",
    "TorqueFields": "TorqueFields",
    "TorqueCSV": "TorqueCsv"
  },
  "config": {
    "TorqueConfigPage": "",
    "TorqueServerLocation": "http://localhost:5000",
    "TorqueGroup": false,
    "TorqueView": false,
    "TorqueCollectionName": "",
    "TorqueWikiKey": "",
    "TorqueNotFoundMessage": "Torque Data Connect Error: Key Not Found",
    "TorqueRaw": false,
    "TorqueRenderToHTML": true,
    "TorqueMultiWikiConfig": false,
    "TorqueCache": false,
    "TorqueWikiUsername": "",
    "TorqueWikiPassword": ""
  },
  "LogTypes": [
    "torque-apiaccess",
    "torque-datachanges"
  ],
  "LogNames": {
    "torque-apiaccess": "torque-apiaccesslog",
    "torque-datachanges": "torque-datachangeslog"
  },
  "LogHeaders": {
    "torque-apiaccess": "torque-apiaccesslog-header",
    "torque-datachanges": "torque-datachangeslog-header"
  },
  "LogActionsHandlers": {
    "torque-apiaccess/*": "LogFormatter",
    "torque-datachanges/*": "LogFormatter"
  },
  "ResourceModules": {
    "ext.torque.css": {
      "styles": [
        "resources/css/TorqueEdit.css",
        "resources/css/TorqueSearch.css",
        "resources/css/TorqueCsv.css"
      ],
      "targets": [
        "desktop",
        "mobile"
      ]
    },
    "ext.torque.js": {
      "scripts": [
         "resources/js/TorqueEdit.js",
         "resources/js/TorqueSearch.js",
         "resources/js/TorqueCsv.js"
      ],
      "dependencies": [
        "mediawiki.api"
      ],
      "messages": [],
      "targets": [
        "desktop",
        "mobile"
      ]
    }
  },
  "ResourceFileModulePaths": {
    "localBasePath": "",
    "remoteExtPath": "Torque"
  },
  "manifest_version": 1
}